{
  "expo": {
    "name": "TechTalent Academy",
    "slug": "aic-triumphants",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/TechTalentAcademy.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "package": "com.triumphants.aic",
      "bundleIdentifier": "com.arrizkyaditha.a.aictriumphants"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Android-icon-v2.png",
        "resizeMode": "contain",
        "package": "com.triumphants.aic"
      },
      "package": "com.arrizkyaditha.a.aictriumphants"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "96d3ab09-a0b3-4373-a7c2-6f94be56170b"
      }
    },
    "owner": "arrizkyaditha.a"
  }
}
